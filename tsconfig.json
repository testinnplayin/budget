{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "alwaysStrict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "removeComments": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "ES6"
    }
}